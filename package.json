{
  "name": "svg-workflow-canvas",
  "version": "1.2.1",
  "description": "Generic workflow builder react component using an SVG canvas",
  "keywords": [
    "workflow",
    "workflow component",
    "workflow builder",
    "workflow designer",
    "pipeline",
    "component",
    "ui",
    "svg",
    "react"
  ],
  "author": "Alex Stanciu <astanciu@gmail.com> (http://github.com/astanciu)",
  "license": "MIT",
  "repository": "astanciu/svg-workflow-canvas",
  "homepage": "https://astanciu.github.io/svg-workflow-canvas",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@svgr/rollup": "^5.4.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.3.0",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.2",
    "gh-pages": "^3.1.0",
    "node-sass": "^7.0.0",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^2.26.8",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.1",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-url": "^3.0.1",
    "typescript": "^4.0.2"
  },
  "files": [
    "dist"
  ]
}
